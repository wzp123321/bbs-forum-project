{
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 120,
  "quoteProps": "as-needed", 
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "requirePragma": false, 
  "insertPragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": false, 
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto"
}
